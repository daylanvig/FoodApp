@typeparam TEntity

<MudTable Items="tableValues" SortLabel="Sort By">
    <HeaderContent>
        @foreach(var tableHeader in TableHeaders)
        {
            <MudTh>
                <MudTableSortLabel SortBy="@tableHeader.SortBy">@tableHeader.Label</MudTableSortLabel>
            </MudTh>
        }
    </HeaderContent>
    <RowTemplate>
        @foreach (var tableTemplate in TableTemplates)
        {
            <MudTd DataLabel="@tableTemplate.Label">@tableTemplate.StringFormatter(context)</MudTd>
        }
    </RowTemplate>
</MudTable>